{
  "babel": {
    "env": {
      "es": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "esnext": {
        "presets": [
          "es2016",
          "es2017",
          "flow",
          "react",
          "stage-0"
        ]
      },
      "node": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "6.0.0"
              }
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "test": {
        "presets": [
          "env",
          "flow",
          "react",
          "stage-0"
        ],
        "plugins": [
          [
            "module-resolver",
            {
              "alias": {
                "skatejs": "./node_modules/skatejs/umd"
              }
            }
          ]
        ]
      },
      "umd": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^20.0.4",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "@skatejs/bore": "^4.0.1",
    "@skatejs/ssr": "^0.15.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "conartist": "^0.12.0",
    "jest-in-case": "^1.0.2",
    "module-alias": "^2.0.1"
  },
  "files": [
    "es/",
    "esnext/",
    "node/",
    "umd/"
  ],
  "main": "node/index.js",
  "module": "es/index.js",
  "esnext": "esnext/index.js",
  "scripts": {
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:esnext": "BABEL_ENV=esnext babel src --out-dir esnext",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "test": "jest",
    "build:umd": "BABEL_ENV=umd rollup -c && BABEL_ENV=umd rollup -c --min",
    "prepublish": "npm run build:es && npm run build:esnext && npm run build:node && npm run build:umd"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "testEnvironment": "@skatejs/ssr/jest"
  },
  "browser": "umd/index.js",
  "dependencies": {
    "@skatejs/val": "^0.3.1",
    "page": "^1.7.1",
    "skatejs": "^5.0.0-beta.3"
  },
  "name": "@skatejs/sk-router",
  "description": "> A web component router that's compatible with code-splitting out of the box.",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skatejs/sk-router.git"
  },
  "keywords": [
    "web",
    "component",
    "router",
    "skatejs"
  ],
  "author": "Trey Shugart <treshugart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skatejs/sk-router/issues"
  },
  "homepage": "https://github.com/skatejs/sk-router#readme",
  "version": "0.0.0"
}
